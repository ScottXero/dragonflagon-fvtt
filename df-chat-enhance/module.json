{
	"id": "df-chat-enhance",
	"title": "DF Chat Enhancements",
	"description": "Various enhancements to the chat features.",
	"version": "5.0.0",
	"author": "No one",
	"compatibility": {
		"minimum": 12,
		"verified": 12
	},
	"relationships": {
