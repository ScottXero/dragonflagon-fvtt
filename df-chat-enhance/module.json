			{
				"id": "lib-wrapper",
				"type": "module",
				"compatibility": {
					"verified": "1.12.14.0"
				}
			},
			{
				"id": "_chatcommands",
				"type": "module",
				"compatibility": {
					"minimum": "2.0.2"
				}
			}
		]
	},
